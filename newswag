batbuy.MouseButton1Click:Connect(function()
    local ItemCost = 265
    local ClickDetector = workspace.Ignored.Shop["[Bat] - $265"].ClickDetector
    local Location = workspace.Ignored.Shop["[Bat] - $265"].ClickDetector.Parent.Head.Position
    OldCFrame = player.Character.HumanoidRootPart.CFrame
    noclip.Text = "clip"
    local function buy()
        task.wait()
        noclip.Text = "clip"
        player.Character.HumanoidRootPart.CFrame = CFrame.new(Location) * CFrame.new(0,-4,0)
        fireclickdetector(ClickDetector)
    end
    repeat buy() until player.Backpack:FindFirstChild("[Bat]") or player.DataFolder.Currency.Value <= ItemCost or player.Character:FindFirstChild("[Bat]")
    noclip.Text = "noclip"
    player.Character.HumanoidRootPart.CFrame = OldCFrame
end)

